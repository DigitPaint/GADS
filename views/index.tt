<div id="ld-app" class="react-grid-layout" data-dashboard="[% dashboard.id %]"
    data-dashboard-endpoint="/[% layout.identifier %]/api/dashboard"
    data-widget-types='["graph", "chart", "table", "bar"]'
>
    [% FOREACH widget IN dashboard.widgets %]
        <div class="ld-widget-container" data-grid='[% widget.grid %]'>
            [% widget.html %]
        </div>
    [% END %]
</div>
